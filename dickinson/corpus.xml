<corpus>
    <poem title="The Rock" author="Charlotte Perkins Stetson" date=""><line/><line/><line>         <tag class="PRP">I</tag> <tag class="VBP">am</tag> <tag class="DT">the</tag> <tag class="NNP">Rock</tag>, <tag class="JJ">presumptuous</tag> <tag class="NNP">Sea</tag>!</line><line>         <tag class="PRP">I</tag> <tag class="VBP">am</tag> <tag class="VBN">set</tag> <tag class="TO">to</tag> <tag class="VB">encounter</tag> <tag class="NN">thee</tag>.</line><line>         <tag class="NNP">Angry</tag> <tag class="CC">and</tag> <tag class="VBD">loud</tag>, <tag class="CC">or</tag> <tag class="VB">gentle</tag> <tag class="CC">and</tag> <tag class="RB">still</tag>,</line><line>         <tag class="PRP">I</tag> <tag class="VBP">am</tag> <tag class="VBN">set</tag> <tag class="RB">here</tag> <tag class="TO">to</tag> <tag class="VB">limit</tag> <tag class="JJ">thy</tag> <tag class="NN">power</tag>, <tag class="CC">and</tag> <tag class="PRP">I</tag> <tag class="MD">will</tag></line><line>         <tag class="PRP">I</tag> <tag class="VBP">am</tag> <tag class="DT">the</tag> <tag class="NNP">Rock</tag> !</line><line/><line/></poem>
</corpus>
